show create table MODULES ;
Table	Create Table
MODULES	CREATE TABLE `MODULES` (
  `MODULE_NAME` VARCHAR(256) NOT NULL,
  `MODULE_VERSION` VARCHAR(20) NOT NULL,
  `MODULE_AUTHOR` VARCHAR(256) NOT NULL,
  `IS_BUILTIN` BOOLEAN NOT NULL,
  `MODULE_LIBRARY` VARCHAR(254) NOT NULL,
  `MODULE_DESCRIPTION` VARCHAR(254) NOT NULL,
  `MODULE_LICENSE` VARCHAR(80) NOT NULL
) ENGINE=FunctionEngine COLLATE = utf8_general_ci REPLICATE = FALSE DEFINER 'SYSTEM'
show create table PLUGINS ;
Table	Create Table
PLUGINS	CREATE TABLE `PLUGINS` (
  `PLUGIN_NAME` VARCHAR(256) NOT NULL,
  `PLUGIN_TYPE` VARCHAR(256) NOT NULL,
  `IS_ACTIVE` BOOLEAN NOT NULL,
  `MODULE_NAME` VARCHAR(256) NOT NULL
) ENGINE=FunctionEngine COLLATE = utf8_general_ci REPLICATE = FALSE DEFINER 'SYSTEM'
